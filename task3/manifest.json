{
    "manifest_version": 2,
    "name": "IngFinder",
    "description": "Quick 'ing' finder",
    "version": "1.0.0",
    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["main.js"]
        }
      ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}